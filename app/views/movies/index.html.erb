<%= button_to 'Log Out', destroy_user_session_path, method: :delete %>
<h2>Welcome <%= current_user.first.name %>!! <h2>

<h1>Movie Genres</h1>
<h3>Which Genre of Movies Do You Want To View?</h3>
<ul>
<% genre = @movies.distinct.pluck(:genre) %>
<% genre.each do |m| %>
  <li>
    <%= link_to m, genre_path(genre: m) %>
  </li>
<% end %>
</ul>
